            sumReview3M = isNaN(sumReview3M) ? "-" : sumReview3M;
            sumReview6M = isNaN(sumReview6M) ? "-" : sumReview6M;
            sumReview12M = isNaN(sumReview12M) ? "-" : sumReview12M;
