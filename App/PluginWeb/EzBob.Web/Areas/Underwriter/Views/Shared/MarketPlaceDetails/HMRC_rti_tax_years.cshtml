<% (function() {
if (m.HmrcData && m.HmrcData.RtiTaxMonths) {
	%><table class="table centered">
	<thead class="box"><tr class="box-title"><th>Month</th><th>Amount Paid</th><th>Amount Due</th></tr></thead><tbody><%
	for (var i = 0; i < m.HmrcData.RtiTaxMonths.length; i++) {
		var oMonth = m.HmrcData.RtiTaxMonths[i];
		%><tr>
			<td><%= EzBob.formatDateMY(oMonth.DateStart) %></td>
			<td class=right><%= (oMonth.AmountPaid.CurrencyCode == "GBP") ? GBPValues(oMonth.AmountPaid.Amount, true) : oMonth.AmountPaid.Amount %></td>
			<td class=right><%= (oMonth.AmountDue.CurrencyCode == "GBP") ? GBPValues(oMonth.AmountDue.Amount, true) : oMonth.AmountDue.Amount %></td>
		</tr><%
	} // for
	%></tbody></table><%
}
else {
	%>No data found.<%
}
})();
%>
