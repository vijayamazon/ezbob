                        </tr>
                    <%}%>
                    <% if (response.PrincipalActivities != undefined) {%>
                        <tr>
                            <td class=narrow-as-possible>Principal activities</td><td><%-response.PrincipalActivities%></td>
                        </tr>
                    <%}%>
                    <% if (response.EarliestKnownDate != undefined) {%>
                        <tr>
                            <td class=narrow-as-possible>Earliest known date</td><td><%-EzBob.formatDate(response.EarliestKnownDate)%></td>
                        </tr>
                    <%}%>
                    <% if (response.DateOwnershipCommenced != undefined) {%>
                        <tr>
                            <td class=narrow-as-possible>Date ownership commenced</td><td><%-EzBob.formatDate(response.DateOwnershipCommenced)%></td>
                        </tr>
                    <%}%>
                    <% if (response.IncorporationDate != undefined) {%>
	                    <tr>
		                    <td class=narrow-as-possible>Incorporation date</td><td><%-EzBob.formatDate(response.IncorporationDate)%></td>
	                    </tr>
                    <%}%>
                    <% if (response.DateOwnershipCeased != undefined) {%>
	                    <tr>
		                    <td class=narrow-as-possible>Date ownership ceased</td><td><%-EzBob.formatDate(response.DateOwnershipCeased)%></td>
