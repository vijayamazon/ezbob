@model dynamic
<script type="text/template" id="product-settings-template">
	@ProductSettings()
</script>

@helper ProductSettings() {
	<div class="alert-block">
		<table class="table squeezed">
			<thead class="box">
				<tr class="box-title">
					<th colspan="3">Product Settings</th>
				</tr>
			</thead>
			<tbody>
				
			</tbody>
		</table>
		<div class="buttons_container">
			<button class="btn btn-primary" name="SaveGeneralSettings"><i class="fa fa-check-circle" />Save</button>
			<button class="btn btn-primary" name="CancelGeneralSettings"><i class="fa fa-refresh" />Reset</button>
		</div>
	</div>
}

