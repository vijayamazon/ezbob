@model dynamic

<script type="text/template" id="add-paypoint-card-template">
    <div class="modal-header">
        <h3 id="myModalLabel">Add PayPoint Card</h3>
    </div>
    <div class="modal-body">
        <form id="paypoint-edit-form">
            <label>Transaction Id</label>
            <input type="text" style="width:300px" id="paypoint-edit-transactionid" name="transactionid">

            <label>Card Last 4 Digits</label>
            <input type="text" class="input-small" id="paypoint-edit-cardno" name="cardno" maxlength="4">
    
            <label>Expire Date</label>
            <input type="text" class="input-small" id="installment-edit-expiredate" name="expiredate" maxlength="5">
        </form>
    </div>
    <div class="pull-right">
        <button class="btn btn-primary">Save changes</button>
    </div>
</script>