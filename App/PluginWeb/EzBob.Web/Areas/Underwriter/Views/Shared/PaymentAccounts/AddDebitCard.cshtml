@model dynamic

<script type="text/template" id="add-paypoint-card-template">
	<div class="modal-body">
		<form id="paypoint-edit-form" class="form-horizontal">
			<div class="row">
				<div class="span6">
					<div class="control-group">
						<label class="control-label">Transaction Id</label>
						<div class="controls">
							<input type="text" class="form-control" id="paypoint-edit-transactionid" name="transactionid">
						</div>
					</div>
					<div class="control-group">
						<label class="control-label">Card Last 4 Digits</label>
						<div class="controls">
							<input type="text" class="form-control" id="paypoint-edit-cardno" name="cardno" maxlength="4">
						</div>
					</div>
					<div class="control-group">
						<label class="control-label">Expire Date</label>
						<div class="controls">
							<input type="text" class="form-control" id="installment-edit-expiredate" name="expiredate" maxlength="5">
						</div>
					</div>
				</div>
			</div>
		</form>
	</div>
	<div class="pull-right">
		<button class="btn btn-primary">Save changes</button>
	</div>
</script>
