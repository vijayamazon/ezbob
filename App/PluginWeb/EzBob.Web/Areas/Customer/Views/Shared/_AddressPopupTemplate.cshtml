<div id="address-popup" title="Address search by Postcode" style="overflow: visible">
    <input class="postCode" name="postCode" type="hidden" />
    <div class="well">
        <ul class="postCodeTextArea" disabled="disabled">
        </ul>
        <div style="margin-left: 90px; height: 45px;">
            <input class="postCodeBtnCancel button orange pull-right" type="button" value="Cancel" ui-event-control-id="address-form:address-cancelled" />
            <input class="postCodeBtnOk button orange pull-right" type="button" value="OK" disabled="disabled" ui-event-control-id="address-form:address-selected" />            
        </div>
    </div>
</div>
