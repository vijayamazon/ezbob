@helper AdditionalCompanyInfo(string prefix)
{
	var prfx = prefix.ToLower();

    <div class="offline">
        <div class="control-group form_field_container">
            <div class="controls">
                <label class="control-label attardi-input">
                    <span>Property owned by company</span>
                    <select id="@string.Format("{0}{1}", prefix, "PropertyOwnedByCompany")" name="PropertyOwnedByCompany" class="select form_field" ui-event-control-id="personal-info:@string.Format(prfx)_property_owned_by_company">
                        <option></option>
                        <option value="true">Yes</option>
                        <option value="false">No</option>
                    </select>
                    <img id="@string.Format("{0}{1}", prefix, "PropertyOwnedByCompanyImage")" class="field_status required" />
                </label>
            </div>
        </div>

        <div class="control-group form_field_container hide additionalCompanyAddressQuestions">
            <div class="controls">
                <label class="control-label attardi-input">
                    <span>Period of occupation</span>
                    <select id="@string.Format("{0}{1}", prefix, "YearsInCompany")" name="YearsInCompany" class="select form_field" ui-event-control-id="personal-info:@string.Format(prfx)_years_in_company">
                        <option></option>
                        <option value="1">1 year</option>
                        <option value="2">2 years</option>
                        <option value="3">More than 3 years</option>
                    </select>
                    <img id="@string.Format("{0}{1}", prefix, "YearsInCompanyImage")" class="field_status required" />
                </label>
            </div>
        </div>

        <div class="control-group form_field_container hide additionalCompanyAddressQuestions">
            <div class="controls">
                <label class="control-label attardi-input">
                    <span>How many month are left for the rent contract</span>
                    <select id="@string.Format("{0}{1}", prefix, "RentMonthsLeft")" name="RentMonthsLeft" class="select form_field" ui-event-control-id="personal-info:@string.Format(prfx)_rent_months_left)">
                        <option></option>
                        <option value="0-6">0-6 months</option>
                        <option value="6-12">6-12 months</option>
                        <option value="12-24">12-24 months</option>
                        <option value="24+">More than 24 months</option>
                    </select>
                    <img id="@string.Format("{0}{1}", prefix, "RentMonthsLeftImage")" class="field_status required" />
                </label>
            </div>
        </div>
    </div>
}

