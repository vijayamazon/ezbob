<script id="employee-count-template" type="text/template">
    <h2 class="form_section">Payroll information</h2>

    <div class="control-group form_field_container">
        <div class="controls">
            <label class="control-label attardi-input">
                <span>Current # of employees</span>
                <input id="EmployeeCount" name="EmployeeCount" type="text" class="form_field numeric" min="0" ui-event-control-id="personal-info:employee_count" />
                <img id="EmployeeCountImage" class="field_status required" />
            </label>
        </div>
    </div>

    <div class="control-group form_field_container">
        <div class="controls">
            <label class="control-label attardi-input">
                <span># of earning (gross salary) over £41,450 per annum</span>
                <input id="TopEarningEmployeeCount" name="TopEarningEmployeeCount" type="text" class="form_field numeric" min="0" ui-event-control-id="personal-info:top_earning_employee_count" />
                <img id="TopEarningEmployeeCountImage" class="field_status required" />
            </label>
        </div>
    </div>

    <div class="control-group form_field_container">
        <div class="controls">
            <label class="control-label attardi-input">
                <span># of earning (gross salary) less than £9,440 per annum</span>
                <input id="BottomEarningEmployeeCount" name="BottomEarningEmployeeCount" type="text" class="form_field numeric" min="0" ui-event-control-id="personal-info:bottom_earning_employee_count" />
                <img id="BottomEarningEmployeeCountImage" class="field_status required" />
            </label>
        </div>
    </div>

    <div class="control-group form_field_container">
        <div class="controls">
            <label class="control-label attardi-input">
                <span>Change in the # of employees in last 12 months</span>
                <select id="EmployeeCountChange" name="EmployeeCountChange" class="form_field select" ui-event-control-id="personal-info:employee_count_change" >
                    <option value=""></option>
                    <option value="-5">Reduced by 5 or more</option>
                    <option value="-4">Reduced by 4</option>
                    <option value="-3">Reduced by 3</option>
                    <option value="-2">Reduced by 2</option>
                    <option value="-1">Reduced by 1</option>
                    <option value="0">No change</option>
                    <option value="1">Increased by 1</option>
                    <option value="2">Increased by 2</option>
                    <option value="3">Increased by 3</option>
                    <option value="4">Increased by 4</option>
                    <option value="5">Increased by 5 or more</option>
                </select>
                <img id="EmployeeCountChangeImage" class="field_status required" />
            </label>
        </div>
    </div>

    <div class="control-group form_field_container">
        <div class="controls">
            <label class="control-label attardi-input">
                <span>Total monthly salary expenditure (inc tax & NI)</span>
                <input id="TotalMonthlySalary" name="TotalMonthlySalary" type="text" class="form_field cashInput" ui-event-control-id="personal-info:total_monthly_salary" />
                <img id="TotalMonthlySalaryImage" class="field_status required" />
            </label>
        </div>
    </div>
</script>

