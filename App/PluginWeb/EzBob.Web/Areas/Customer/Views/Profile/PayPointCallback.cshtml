@using Newtonsoft.Json
@model dynamic

@{
}
<h2>Trying to add debit card. Please wait.</h2>

@section Js
{
    <script type="text/javascript">
        $(function () {
            window.opener.DebitCardAdded(@Html.Raw(JsonConvert.SerializeObject(Model)));
            self.close();
        });
    </script>
}