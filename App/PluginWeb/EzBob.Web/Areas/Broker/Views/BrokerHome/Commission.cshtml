@model dynamic

<div class="section-dashboard-commission">
    <div class="linked_bank" style="display: none;">
        <div class="profile-headlines">
            <h3>Account linked</h3>
            <h5 class="header-message">Automatic payment will be wired to the account.</h5>
        </div>
        <div class="d-widgets">
            <div class="d-widget">
                <h3>Loans</h3>
                <dl>
                    <dt>Comission:</dt>
                    <dd class="broker-commission" ></dd>
                    <dt>Approved:</dt>
                    <dd class="broker-approved">---</dd>
                </dl>
                <button class="button btn-grey pull-right btn-wide add-bank">Update bank account</button>
            </div>
        </div>
    </div>
    <div class="not_linked_bank" style="display: none;">
        <div class="profile-headlines">
            <h3><span class="broker-name"></span>, link account & get payed!</h3>
            <h5 class="header-message">Link account and receive commission automatically.</h5>
        </div>
        <div class="d-widgets">
            <div class="d-widget">
                <h3>Loans</h3>
                <dl>
                    <dt>Comission:</dt>
                    <dd class="dashes">---</dd>
                    <dt>Approved:</dt>
                    <dd class="broker-approved"></dd>
                </dl>
                <button class="button btn-green pull-right btn-wide add-bank">Link bank account</button>
            </div>
        </div>
    </div>
</div>
