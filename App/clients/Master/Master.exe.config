<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="PatronConfig" type="PatronLib.PatronConfig, PatronLib"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="SpecificSettings" type="Scorto.ConnectionsManager.SSSConfigSection,WindowsClientLib" allowDefinition="Everywhere" allowExeDefinition="MachineToApplication" restartOnExternalChanges="true"/>
      <section name="Patron.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Patron.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <appSettings>
    <add key="pin" value=""/>
  </appSettings>

  <applicationSettings>
    <SpecificSettings>
      <ConnectionProfiles ShowProfiles="true" DefaultProfileName="Main Connection">
        <add Name="Main Connection" Address="http://localhost/303/PatronServer/Service.asmx"/>
      </ConnectionProfiles>
    </SpecificSettings>
    <Patron.Properties.Settings>
      <setting name="AppCulture" serializeAs="String">
        <value>en-US</value>
      </setting>
    </Patron.Properties.Settings>
  </applicationSettings>

  <PatronConfig>
    <Plugins>
      <Plugin Name="Default" Path=".\DefaultSigningProvider.dll"/>
      <Plugin Name="Strategies module" Path=".\Strategies.dll"></Plugin>
      <Plugin Name="Nodes module" Path=".\Nodes.dll"></Plugin>
      <Plugin Name="Credit product module" Path=".\CreditProduct.dll"></Plugin>
      <Plugin Name="Export templates module" Path=".\Templates.dll"></Plugin>
      <Plugin Name="Reports module" Path=".\Reports.dll"></Plugin>
      <Plugin Name="Behavioral Strategies module" Path=".\BehavioralStrategyStart.dll"></Plugin>
      <Plugin Name="DataSources module" Path="Scorto.Patron.Plugin.DataSourceDefinition.dll" />
	  <Plugin Name="EmailAccounts module" Path="EmailAccounts.dll" />
      <Plugin Name="StrategyAreas module" Path="Scorto.Patron.Plugin.BehavioralStrategyAreas.dll" />
      <Plugin Name="StrategyScheduler module" Path="BehavioralStrategyScheduler.dll" />
      <Plugin Name="ScoringModels module" Path="ScoringModels.dll" />
      <Plugin Name="Workplaces module" Path="Workplaces.dll" />
      <Plugin Name="Business entities module" Path="BusinessEntities.dll" />
    </Plugins>
  </PatronConfig>
  <userSettings>
    <Patron.Properties.Settings>
      <setting name="AppName" serializeAs="String">
        <value>Master</value>
      </setting>
      <setting name="AppEdition" serializeAs="String">
        <value>Standard edition</value>
      </setting>
    </Patron.Properties.Settings>
  </userSettings>
  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value=".\log\master.log"/>
      <appendToFile value="true"/>
      <maximumFileSize value="1000KB"/>
      <maxSizeRollBackups value="3"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] [%5level] (%file:%line) %logger - %message%newline"/>
      </layout>
    </appender>
    <root>
      <level value="ERROR"/>
      <appender-ref ref="RollingFile"/>
    </root>
  </log4net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
