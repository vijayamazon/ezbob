<?xml version="1.0"?>
<configuration>

  <configSections>
    <section name="FormsDesignerConfig" type="FormsDesignerLib.FormsDesignerConfig, FormsDesignerLib"/>
    <section name="appAssemblies" type="System.Configuration.NameValueSectionHandler"/>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="SpecificSettings" type="Scorto.ConnectionsManager.SSSConfigSection,WindowsClientLib" allowDefinition="Everywhere" allowExeDefinition="MachineToApplication" restartOnExternalChanges="true"/>
      <section name="NodeDesigner.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="NodeDesigner.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <log4net>
    <appender name="ColoredConsoleAppender" type="log4net.Appender.ColoredConsoleAppender">
      <mapping>
        <level value="FATAL"/>
        <foreColor value="Red, HighIntensity"/>
      </mapping>
      <mapping>
        <level value="ERROR"/>
        <foreColor value="Red"/>
      </mapping>
      <mapping>
        <level value="WARN"/>
        <foreColor value="Yellow"/>
      </mapping>
      <mapping>
        <level value="INFO"/>
        <foreColor value="White"/>
      </mapping>
      <mapping>
        <level value="DEBUG"/>
        <foreColor value="Green"/>
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-8date{HH:mm:ss} [%thread][%logger] %-5level- %message%newline"/>
      </layout>
    </appender>

    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="NodeDesigner.txt"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level- %message%newline"/>
      </layout>
    </appender>

    <root>
      <level value="ALL"/>
      <appender-ref ref="ColoredConsoleAppender"/>
    </root>
  </log4net>

  <applicationSettings>
    <SpecificSettings>
      <ConnectionProfiles ShowProfiles="true" DefaultProfileName="Main Connection">
        <add Name="Main Connection" Address="http://localhost/303/FormsDesignerServer/Service.asmx"/>
      </ConnectionProfiles>
    </SpecificSettings>
    <NodeDesigner.Properties.Settings>
      <setting name="Build" serializeAs="String">
        <value/>
      </setting>
      <setting name="AppCulture" serializeAs="String">
        <value>uk-UA</value>
      </setting>
      <setting name="PrintMode" serializeAs="String">
        <value>DevExpress</value>
      </setting>
      <setting name="GenerateGuid" serializeAs="String">
        <value>True</value>
      </setting>
    </NodeDesigner.Properties.Settings>
  </applicationSettings>
  <userSettings>
    <NodeDesigner.Properties.Settings>
      <setting name="Editor" serializeAs="String">
        <value>Advanced</value>
      </setting>
      <setting name="AppName" serializeAs="String">
        <value>Forms Designer</value>
      </setting>
      <setting name="AppEdition" serializeAs="String">
        <value>Standard edition</value>
      </setting>
      <setting name="TrackChanges" serializeAs="String">
        <value>True</value>
      </setting>
    </NodeDesigner.Properties.Settings>
  </userSettings>

  <FormsDesignerConfig>
    <Plugins>
      <Plugin Name="Default" Path=".\DefaultSigningProvider.dll"/>
    </Plugins>
  </FormsDesignerConfig>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
