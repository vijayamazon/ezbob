<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
		<section name="syscache2" type="NHibernate.Caches.SysCache2.SysCacheSection, NHibernate.Caches.SysCache2" />
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>

	<connectionStrings configSource="connection-strings.xml" />

	<log4net>
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="c:\temp\logs\EzService.log" />
			<appendToFile value="true" />
			<rollingStyle value="Composite" />
			<datePattern value="yyyy-MM-dd" />
			<maxSizeRollBackups value="1000" />
			<maximumFileSize value="100MB" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %message%newline" />
			</layout>
		</appender>

		<appender name="RollingLogFileCompact" type="log4net.Appender.RollingFileAppender">
			<file value="c:\temp\logs\EzService.compact.log" />
			<appendToFile value="true" />
			<rollingStyle value="Composite" />
			<datePattern value="yyyy-MM-dd" />
			<maxSizeRollBackups value="1000" />
			<maximumFileSize value="100MB" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %message%newline" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="INFO" />
				<levelMax value="FATAL" />
			</filter>
		</appender>

		<appender name="MarketplacesRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="c:\temp\logs\EzService.Marketplaces.log" />
			<appendToFile value="true" />
			<rollingStyle value="Composite" />
			<datePattern value="yyyy-MM-dd" />
			<maxSizeRollBackups value="1000" />
			<maximumFileSize value="100MB" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %message%newline" />
			</layout>

			<filter type="log4net.Filter.LoggerMatchFilter">
				<acceptOnMatch value="true" />
				<LoggerToMatch value="EzBob.CommonLib" />
			</filter>

			<filter type="log4net.Filter.DenyAllFilter" />
		</appender>

		<appender name="MailAppenderWithSSL" type="Log4NetCustomAppenders.MailAppenderWithSSL,Log4NetCustomAppenders">
			<to type="log4net.Util.PatternString" value="%property{ErrorEmailRecipient}" />
			<from value="ezbobdev@gmail.com" />
			<subject type="log4net.Util.PatternString" value="#proderror %property{MailSubject}" />
			<smtpHost value="smtp.gmail.com" />
			<authentication value="Basic" />
			<username value="ezbobdev@gmail.com" />
			<password value="not1977NOT" />
			<port value="587" />
			<lossy value="false" />
			<bufferSize value="1" />
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="ERROR" />
				<levelMax value="FATAL" />
			</filter>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %5level UserId = %5property{UserId} %property{Controller}.%property{Action} %logger %message%newline" />
			</layout>
		</appender>

		<root>
			<level value="ALL"/>
			<appender-ref ref="RollingLogFileAppender"/>
			<appender-ref ref="RollingLogFileCompact"/>
			<appender-ref ref="MarketplacesRollingLogFileAppender"/>
			<appender-ref ref="MailAppenderWithSSL"/>
		</root>

		<logger name="NHibernate">
			<level value="WARN" />
		</logger>
		<logger name="NHibernate.SQL">
			<level value="WARN" />
		</logger>
		<logger name="AbstractBatcher.SQL">
			<level value="WARN" />
		</logger>
	</log4net>

	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="CertPortBinding" closeTimeout="00:01:00" openTimeout="00:01:00"
                    receiveTimeout="00:10:00" sendTimeout="00:01:00" bypassProxyOnLocal="false"
                    hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="524288"
                    maxBufferSize="65536000" maxReceivedMessageSize="65536000"
                    textEncoding="utf-8" useDefaultWebProxy="true" messageEncoding="Text">
					<readerQuotas maxDepth="32" maxStringContentLength="65536000"
                        maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
					<security mode="Transport">
						<transport clientCredentialType="Certificate" proxyCredentialType="None"
                            realm="" />
						<message clientCredentialType="UserName" />
					</security>
				</binding>
			</basicHttpBinding>
		</bindings>

		<behaviors>
			<endpointBehaviors>
				<!--land registry test-->
				<behavior name="testCert">
					<clientCredentials>
						<clientCertificate findValue="62 ee f2 5f 7c 6c 5b d3 69 07 32 49 c5 55 56 97 fb bc ee fc" x509FindType="FindByThumbprint" storeLocation="LocalMachine" storeName="My"/>
						<serviceCertificate>
							<authentication certificateValidationMode="PeerTrust"/>
						</serviceCertificate>
					</clientCredentials>
				</behavior>
				<!--land registry prod-->
				<behavior name="prodCert">
					<clientCredentials>
						<clientCertificate findValue="76 e9 a4 91 45 57 bc 1d 46 3e f2 79 3e 46 79 d8 09 9e a5 49" x509FindType="FindByThumbprint" storeLocation="LocalMachine" storeName="My"/>
						<serviceCertificate>
							<authentication certificateValidationMode="PeerTrust"/>
						</serviceCertificate>
					</clientCredentials>
				</behavior>
			</endpointBehaviors>
		</behaviors>
		<client>
			<endpoint address="https://bgtest.landregistry.gov.uk/b2b/ECBG_StubService/EnquiryByPropertyDescriptionV2_0WebService"
                behaviorConfiguration="testCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LREnquiryServiceTestNS.PropertyDescriptionEnquiryV2_0Service"
                name="PropertyDescriptionV2_0WSImplPort1" />
			<endpoint address="https://bgtest.landregistry.gov.uk/b2b/BGStubService/OfficialCopyWithSummaryV2_1WebService"
                behaviorConfiguration="testCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LRRESServiceTestNS.OCWithSummaryV2_1Service"
                name="OCWithSummaryV2_1WSImplPort1" />
			<endpoint address="https://bgtest.landregistry.gov.uk/b2b/ECBG_StubService/EnquiryByPropertyDescriptionV2_0PollWebService"
                behaviorConfiguration="testCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LREnquiryPollServiceTestNS.PropertyDescriptionEnquiryV2_0PollService"
                name="PropertyDescriptionEnquiryV2_0PollRequestWSImplPort1" />
			<endpoint address="https://bgtest.landregistry.gov.uk/b2b/BGStubService/OfficialCopyWithSummaryV2_0PollRequestWebService"
                behaviorConfiguration="testCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LRRESPollServiceTestNS.OCWithSummaryV2_0PollService"
                name="OCWithSummaryV2_0PollRequestWSImplPort" />
			<endpoint address="https://businessgateway.landregistry.gov.uk/b2b/BGSoapEngine/EnquiryByPropertyDescriptionV2_0WebService"
                behaviorConfiguration="prodCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LREnquiryServiceNS.PropertyDescriptionEnquiryV2_0Service"
                name="PropertyDescriptionV2_0WSImplPort" />
			<endpoint address="https://businessgateway.landregistry.gov.uk/b2b/BGSoapEngine/EnquiryByPropertyDescriptionV2_0PollWebService"
                behaviorConfiguration="prodCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LREnquiryPollServiceNS.PropertyDescriptionEnquiryV2_0PollService"
                name="PropertyDescriptionEnquiryV2_0PollRequestWSImplPort" />
			<endpoint address="https://businessgateway.landregistry.gov.uk/b2b/BGSoapEngine/OfficialCopyWithSummaryV2_1WebService"
                behaviorConfiguration="prodCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LRResServiceNS.OCWithSummaryV2_1Service"
                name="OCWithSummaryV2_1WSImplPort" />
			<endpoint address="https://businessgateway.landregistry.gov.uk/b2b/BGSoapEngine/OfficialCopyWithSummaryV2_0PollRequestWebService"
                behaviorConfiguration="prodCert" binding="basicHttpBinding"
                bindingConfiguration="CertPortBinding" contract="LRResPollServiceNS.OCWithSummaryV2_0PollService"
                name="OCWithSummaryV2_0PollRequestWSImplPort" />
		</client>
	</system.serviceModel>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="HtmlAgilityPack" publicKeyToken="bd319b19eaf3b43a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-1.4.6.0" newVersion="1.4.6.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

	<syscache2>
		<cacheRegion name="DbStrings" relativeExpiration="12000" />
		<cacheRegion name="Longest" relativeExpiration="3" />
		<cacheRegion name="LongTerm" relativeExpiration="3" />
		<cacheRegion name="VeryShort" relativeExpiration="3" />
	</syscache2>
</configuration>