            <clientCertificate findValue="76 e9 a4 91 45 57 bc 1d 46 3e f2 79 3e 46 79 d8 09 9e a5 49" x509FindType="FindByThumbprint" storeLocation="CurrentUser" storeName="My" />
            <serviceCertificate>
